# format = "[ $path]($style)[$read_only]($read_only_style) "

[directory]
truncation_length = 3
truncation_symbol = "â€¦/"
home_symbol = "ó°‹œ ~"
read_only_style = "197"
read_only = " ï€£ "
format = "[$path]($style)[$read_only]($read_only_style) "

# Before all the version info (python, nodejs, php, etc.)
[git_branch]
symbol = "ï˜ "
style = "purple"
format = "[ $symbol](white)[$branch ]($style)"

[git_status]
style = "white"
ahead = "â‡¡${count}"
diverged = "â‡•â‡¡${ahead_count}â‡£${behind_count}"
behind = "â‡£${count}"
deleted = "x"

# Last param in the first line/row
[cmd_duration]
min_time = 1
format = "took [$duration]($style)"
disabled = false


## SECOND LINE/ROW: Prompt
# Somewhere at the beginning
# Prompt: optional param 1
[time]
format = "$time($style) "
time_format = "%T"
style = "bright-white"
disabled = false

# Prompt: param 2 â””â”€
[character] 
success_symbol = "[â¯](green)"
error_symbol = "[â¯](red)"

# SYMBOLS
[status]
symbol = "ğŸ”´"
format = '[\[$symbol$status_common_meaning$status_signal_name$status_maybe_int\]]($style)'
map_symbol = true
disabled = false

[battery]
format = '[$symbol$percentage]($style) '
discharging_symbol = "ó°¹ "
# full_symbol = "ó°¹ "
# charging_symbol = "ó°‚„ " 
# discharging_symbol = "ó°‚ƒ "
disabled = false

[[battery.display]]  # "bold red" style when capacity is between 0% and 10%
threshold = 20
style = "red"

[[battery.display]]  # "bold yellow" style when capacity is between 15% and 50%
threshold = 50
style = "bright-yellow"

[[battery.display]]  # "green" style when capacity is between 50% and 100%
threshold = 100
style = "green"

